{
  "name": "easy-intercept",
  "version": "0.0.1",
  "description": "Utility for intercepting Javascript",
  "author": "Buddhike de Silva",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel lib/ -d dist/ --source-maps",
    "eslint": "eslint src test",
    "test": "istanbul cover node_modules/.bin/_mocha -- -u exports --compilers js:babel-register -R spec -r test/setup.js test/*{,*/}*.js",
    "watch": "nodemon --ignore dist/ --ignore node_modules/ --exec 'npm run eslint && npm run test && npm run build'",
    "start": "npm run watch"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "data.maybe": "^1.2.2",
    "ramda": "^0.22.1"
  }
}
